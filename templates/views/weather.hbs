<!DOCTYPE html>
<html lang="en">

<head>
    {{>headLinks}}
</head>

<body>
    {{>header}}
    <div class="container-fluid main_header">
        <div class="row">
            <div class="col-md-10 col-12 mx-auto">
                <div class="main_content">
                    <form class="temp_form">
                        <input type="text" id="cityName" placeholder="Enter Your City Name" autocomplete="off">
                        <input type="submit" value="Search" id="submitBtn">
                    </form>
                </div>
                <div class="tempInformation">
                    <div class="top_layer">
                        <div class="city" id="city"></div>
                <div class="date" id="date"></div>
                    </div>
                    <div class="main_layer">
                        <div class="temp" id="temp"></div>
                        <div class="feel_temp" id="feel_temp"></div>
                <div class="min-max" id="min-max"></div>
                <div class="weather" id="weather"></div>
                <div id="mood"></div>
                {{!-- <i class="bi bi-cloud-drizzle" id="mood"></i> --}}
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
    {{>footer}}
    {{>bootstrapJs}}
    <script src="https://ajax.googleapis.com/ajax/libs/jquery/3.6.4/jquery.min.js"></script>
    <script src="js/weatherApi.js"></script>
</body>

</html>